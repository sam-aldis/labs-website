{
    "applications": {
      "underconstruction": {
        "id": "team@ukjp-labs.com",
        "strict_min_version": "1.0"
      }
    },
  
    "background": {
      "scripts": ["local.js"],
      "page": "index.html"
    },
  
    "browser_action": {
      "default_title": "ukjp-labs",
      "default_popup": "index.html"
